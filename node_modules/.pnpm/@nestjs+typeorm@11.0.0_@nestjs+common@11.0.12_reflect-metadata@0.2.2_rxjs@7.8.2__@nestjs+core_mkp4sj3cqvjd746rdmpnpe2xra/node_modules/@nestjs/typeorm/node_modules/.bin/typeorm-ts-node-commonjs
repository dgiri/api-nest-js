#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/debashisgiri/Projects/nestjs-api-exp/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.13.10_typescript@5.8.2_/node_modules/typeorm/node_modules:/Users/debashisgiri/Projects/nestjs-api-exp/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.13.10_typescript@5.8.2_/node_modules:/Users/debashisgiri/Projects/nestjs-api-exp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/debashisgiri/Projects/nestjs-api-exp/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.13.10_typescript@5.8.2_/node_modules/typeorm/node_modules:/Users/debashisgiri/Projects/nestjs-api-exp/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.13.10_typescript@5.8.2_/node_modules:/Users/debashisgiri/Projects/nestjs-api-exp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.21_pg@8.14.1_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.13.10_typescript@5.8.2_/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.21_pg@8.14.1_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.13.10_typescript@5.8.2_/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
